{
  "buildCommand": "npm run build",
  "public": true,
  "outputDirectory": "dist",
  "framework": "vite",
  "headers": [
    {
      "source": "/images/performers/(.*)\\.mp4",
      "headers": [
        { "key": "Content-Type", "value": "video/mp4" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Cache-Control", "value": "public, max-age=604800" }
      ]
    },
    {
      "source": "/images/performers/(.*)\\.mov",
      "headers": [
        { "key": "Content-Type", "value": "video/quicktime" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Cache-Control", "value": "public, max-age=604800" }
      ]
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/images/performers/(.*)\\.mp4$",
      "dest": "/images/performers/$1.mp4"
    },
    {
      "src": "/images/performers/(.*)\\.mov$",
      "dest": "/images/performers/$1.mov"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}